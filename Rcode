#Bootstrap

s<-read.csv(file="Standard.csv", header=TRUE, sep=",")

s$Gender.f<-factor(s$Gender)

logit <- glm(Bin_CSR_Adap_sell ~ EmployeeCSRimpXCustomCSRsupp + OrgIDXFit + Ability_to_construe + Gender.f, data=s, family = "binomial")

summary(logit)

library(robust)

B<-glmRob(Bin_CSR_Adap_sell ~ EmployeeCSRimpXCustomCSRsupp + OrgIDXFit + Ability_to_construe + Gender.f, family = binomial, data=s, method="mallows")

summary(B)

c<-glmRob(Bin_CSR_Adap_sell ~ EmployeeCSRimpXCustomCSRsupp + OrgIDXFit + Ability_to_construe + Gender.f, family = binomial, data=s, method="misclass")

summary(c)

library(car)

First<-Boot(logit,R=999, method="case")

summary(First)

confint(First)
